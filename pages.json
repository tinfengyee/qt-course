{
  "pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
    {
      "path": "pages/index/index",
      "style": {
        "navigationStyle": "custom"
      }
    }, {
      "path": "pages/home/home",
      "style": {
        "navigationStyle": "custom"
      }
    }, {
      "path": "pages/my/my",
      "style": {}
    }
  ],
  "globalStyle": {
    "navigationBarTextStyle": "white",
    "navigationBarTitleText": "",
    "navigationBarBackgroundColor": "#BE1F2D",
    "backgroundColor": "#F8F8F8"
  },
  "tabBar": {
    "color": "#909399",
    "selectedColor": "#303133",
    "borderStyle": "black",
    "backgroundColor": "#ffffff",
    "list": [{
      "pagePath": "pages/home/home",
      "iconPath": "static/img/tabbar/home.png",
      "selectedIconPath": "static/img/tabbar/home.png",
      "text": "首頁"
    }, {
      "pagePath": "pages/my/my",
      "iconPath": "static/img/tabbar/my.png",
      "selectedIconPath": "static/img/tabbar/my.png",
      "text": "我的"
    }]
  },
  "easycom": {
    "^r-(.*)": "@/rview-ui/components/r-$1/r-$1.vue",
    "^c-(.*)": "@/components/course/c-$1/c-$1.vue"
  }
}
